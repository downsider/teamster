{
  "name": "silktide/teamster",
  "description": "An implementation of the Thread Pool pattern, for running a suite of console commands continuously",
  "license": "proprietary",
  "homepage": "https://github.com/silktide/teamster",
  "repositories": [
    {
      "type": "composer",
      "url": "http://satis.silktide.com"
    }
  ],
  "require": {
    "php": ">=5.3.0",
    "silktide/syringe": "0.1.*",
    "downsider/puzzle-di": "0.1.*",
    "symfony/console": "2.6.*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*",
    "mockery/mockery": "0.9.*",
    "mikey179/vfsStream": "1.4.*"
  },
  "autoload": {
    "psr-4": {
      "Silktide\\Teamster\\": "src",
      "Silktide\\Teamster\\Test\\": "test"
    }
  },
  "extra": {
    "downsider-puzzle-di": {
      "silktide/syringe": {
        "path": "config/syringe.json"
      }
    }
  }
}